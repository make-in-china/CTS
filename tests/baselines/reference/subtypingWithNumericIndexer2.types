// Derived type indexer must be subtype of base type indexer

interface Base { foo: string; }
>Base : Base
>foo : string
>类型别名 :=> 文字

interface Derived extends Base { bar: string; }
>Derived : Derived
>Base : Base
>bar : string
>类型别名 :=> 文字

interface Derived2 extends Derived { baz: string; }
>Derived2 : Derived2
>Derived : Derived
>baz : string
>类型别名 :=> 文字

interface A {
>A : A

    [x: number]: Derived;
>x : number
>类型别名 :=> 数字
>Derived : Derived
}

interface B extends A {
>B : B
>A : A

    [x: number]: Base; // error
>x : number
>类型别名 :=> 数字
>Base : Base
}

interface B2 extends A {
>B2 : B2
>A : A

    [x: number]: Derived2; // ok
>x : number
>类型别名 :=> 数字
>Derived2 : Derived2
}

module Generics {
>Generics : any
>类型别名 :=> 任意

    interface A<T extends Derived> {
>A : A<T>
>T : T
>Derived : Derived

        [x: number]: T;
>x : number
>类型别名 :=> 数字
>T : T
    }

    interface B extends A<Base> {
>B : B
>A : A<T>
>Base : Base

        [x: number]: Derived; // error
>x : number
>类型别名 :=> 数字
>Derived : Derived
    }

    interface B2 extends A<Derived> {
>B2 : B2
>A : A<T>
>Derived : Derived

        [x: number]: Derived2; // ok
>x : number
>类型别名 :=> 数字
>Derived2 : Derived2
    }

    interface B3<T extends Derived> extends A<T> {
>B3 : B3<T>
>T : T
>Derived : Derived
>A : A<T>
>T : T

        [x: number]: Base; // error
>x : number
>类型别名 :=> 数字
>Base : Base
    }

    interface B4<T extends Derived> extends A<T> {
>B4 : B4<T>
>T : T
>Derived : Derived
>A : A<T>
>T : T

        [x: number]: Derived; // error
>x : number
>类型别名 :=> 数字
>Derived : Derived
    }

    interface B5<T extends Derived2> extends A<T> {
>B5 : B5<T>
>T : T
>Derived2 : Derived2
>A : A<T>
>T : T

        [x: number]: Derived2; // error
>x : number
>类型别名 :=> 数字
>Derived2 : Derived2
    }
}
