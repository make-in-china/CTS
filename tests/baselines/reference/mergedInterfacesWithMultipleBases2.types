// merged interfaces behave as if all extends clauses from each declaration are merged together
// no errors expected

class C {
>C : C

    a: number;
>a : number
>类型别名 :=> 数字
}

class C2 {
>C2 : C2

    b: number;
>b : number
>类型别名 :=> 数字
}

class C3 {
>C3 : C3

    c: string;
>c : string
>类型别名 :=> 文字
}

class C4 {
>C4 : C4

    d: string;
>d : string
>类型别名 :=> 文字
}


interface A extends C, C3 {
>A : A
>C : C
>C3 : C3

    y: string;
>y : string
>类型别名 :=> 文字
}

interface A extends C2, C4 {
>A : A
>C2 : C2
>C4 : C4

    z: string;
>z : string
>类型别名 :=> 文字
}

class D implements A {
>D : D
>A : A

    a: number;
>a : number
>类型别名 :=> 数字

    b: number;
>b : number
>类型别名 :=> 数字

    c: string;
>c : string
>类型别名 :=> 文字

    d: string;
>d : string
>类型别名 :=> 文字

    y: string;
>y : string
>类型别名 :=> 文字

    z: string;
>z : string
>类型别名 :=> 文字
}

var a: A;
>a : A
>A : A

var r = a.a;
>r : number
>类型别名 :=> 数字
>a.a : number
>类型别名 :=> 数字
>a : A
>a : number
>类型别名 :=> 数字

// generic interfaces in a module
module M {
